<?xml version="1.0"?>
<database xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="zed" xsi:noNamespaceSchemaLocation="http://static.spryker.com/schema-01.xsd" namespace="Orm\Zed\CustomerCoefficient\Persistence" package="src.Orm.Zed.CustomerCoefficient.Persistence">

    <table name="spy_coefficient" idMethod="native" phpName="SpyCoefficient" identifierQuoting="true">

        <column name="id_coefficient" required="true" primaryKey="true" autoIncrement="true" type="INTEGER"/>
