<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd" >

    <transfer name="Customer">
        <property name="phone" type="string" />
        <property name="isDirty" type="bool" />
        <property name="organizationRole" type="string" />
        <property name="idOrganization" type="int" />
        <property name="organizationName" type="string" />
    </transfer>

    <transfer name="CustomerOverviewRequest">
        <property name="customer" type="Customer" />
        <property name="orderList" type="OrderList" />
    </transfer>

    <transfer name="CustomerOverviewResponse">
        <property name="orderList" type="OrderList" />
        <property name="isSubscribed" type="bool" />
    </transfer>

    <transfer name="Customers">
        <property name="customers" type="Customer[]" singular="customer"/>
    </transfer>

</transfers>
