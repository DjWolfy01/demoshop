<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://static.spryker.com http://static.spryker.com/transfer-01.xsd" >

    <transfer name="Customer">
        <property name="pricingPercentage" type="float" />
        <property name="pricingFactors" type="PricingFactor[]" singular="pricingFactor" />
    </transfer>

    <transfer name="PricingFactor">
        <property name="pricingFactorType" type="PricingFactorType" />
        <property name="value" type="string" />
        <property name="percentage" type="float" />
    </transfer>

    <transfer name="PricingFactorType">
        <property name="name" type="string" />
    </transfer>

</transfers>
